{
  "extends": "airbnb/base",
  "env": {
    "node": true
  },
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "new-cap": 0,
    "no-param-reassign": 0,
    "no-path-concat": 1,
    "no-unused-vars": 0,
    "object-shorthand": 0,
    "prefer-template": 0,
    "valid-jsdoc": 1
  }
}
